{
  "name": "playwright-website-scraper",
  "version": "1.0.0",
  "description": "A specialized web scraper for specific websites using Playwright",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js --mode test",
    "start:product": "node src/main.js --mode production",
    "dist": "pkg . --no-bytecode --out-path out --targets node18-win-x64 --public-packages axios,winston,winston-daily-rotate-file "
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "minimist": "^1.2.8",
    "playwright": "^1.39.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.3",
    "jest": "^29.7.0",
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bin": "src/main.js"
}